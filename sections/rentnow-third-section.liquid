<div class="rentnow-third-section">

  <div class="rentnow-main-container3">

    <div class="rentnow-header3-container1">
      <h1>Selected Dates:</h1>
      <p>17/09/23 - 25/09/23</p>
      <p>(9days)</p>
    </div>

    <div class="rentnow-header3-container2">
      <h1>Rental Cost:</h1>
      <p>$1/day  x  9 days =</p>
      <p>$9</p>
    </div>

    <div class="rentnow-header3-container3">
      <h1>Collection Method</h1>
      <div class="rentnow-subheader-container6">
        <div class="rentnow-subheader-container7">
          <svg xmlns="http://www.w3.org/2000/svg" width="60" height="60" viewBox="0 0 60 60" fill="none">
            <circle cx="30" cy="30" r="27.5" stroke="#0D2610" stroke-width="5"/>
          </svg>
          <img class= "rentnow-selfcollection-image" src="{{ 'rentnow-selfcollection.svg' | asset_url }}" />
          <p>Self-Collect ( Free )</p>
        </div>

        <div class="rentnow-subheader-container8">
          <svg xmlns="http://www.w3.org/2000/svg" width="60" height="60" viewBox="0 0 60 60" fill="none">
            <circle cx="30" cy="30" r="27.5" stroke="#0D2610" stroke-width="5"/>
          </svg>
          <img class= "rentnow-delivery-image" src="{{ 'rentnow-delivery.svg' | asset_url }}" />
          <p>Delivery + $20</p>
        </div>
      </div>
    <div>

    <div class="rentnow-header3-container4">
    </div>

  </div>
</div>

